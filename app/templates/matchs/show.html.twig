<!DOCTYPE html>

<title>Matchs</title>

{% block body %}
    <h1>Matchs</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Description</th>
                <td>{{ match.description }}</td>
            </tr>
            <tr>
                <th>Score</th>
                <td>{{ match.score }}</td>
            </tr>
            <tr>
                <th>Score_detail</th>
                <td>{{ match.scoreDetail }}</td>
            </tr>
            <tr>
                <th>TypeScore</th>
                <td>{{ match.typeScore }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ match.id }}</td>
            </tr>
            <tr>
                <th>Uuid</th>
                <td>{{ match.uuid }}</td>
            </tr>
            <tr>
                <th>Published</th>
                <td>{{ match.published ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ match.createdAt ? match.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ match.updatedAt ? match.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('matchs_index') }}">back to list</a>

    <a href="{{ path('matchs_edit', {'id': match.id}) }}">edit</a>

    {{ include('matchs/_delete_form.html.twig') }}
{% endblock %}
